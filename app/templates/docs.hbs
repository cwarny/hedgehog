<div class="row">
	<div class="col-md-3">
		<ul>
			{{#each ann in annotations}}
				<li><i class="fa fa-{{ann.iconType}}"></i>{{ann.text}}</li>
			{{/each}}
		</ul>
	</div>
	<div class="col-md-6">
		{{#if q}}
			{{#each doc in this}}
				{{#link-to "doc" doc tagName="p" class="search-result well"}}
					{{{doc.highlight}}}
				{{/link-to}}
			{{/each}}
		{{/if}}
	</div>
	<div class="col-md-3">
	</div>
</div>